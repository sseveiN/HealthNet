{% extends "main_template.html" %}

{% block title %} Edit Info {% endblock title %}

{% block content %}

{% load bootstrap3 %}
<form action="" method="post" class="form">
    {% csrf_token %}
    {% bootstrap_form edit_info %}
    {% buttons %}
        <button type="submit" class="btn btn-primary">
            {% bootstrap_icon "star" %} Edit User
        </button>
    {% endbuttons %}
</form>

{% endblock content %}