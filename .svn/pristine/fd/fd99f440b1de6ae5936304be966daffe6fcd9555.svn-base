<!DOCTYPE html>
<html lang="en">

{% load staticfiles %}

<head>
    <meta charset="UTF-8">
    <title>HealthNet - {% block title %}{% endblock title %}</title>

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/t/bs-3.3.6/jq-2.2.0,dt-1.10.11,af-2.1.1/datatables.min.css"/>
    <link rel="stylesheet" type="text/css" href="{% static "css/main.css" %}"/>

    {% block css %}
    {% endblock css %}

    {% block headscripts %}
    {% endblock headscripts %}
</head>

<body>
    {% include "fragments/navigation.html" %}

    <div class="container">
    {% for message in messages %}
        <div class="alert alert-{{ message.tags }}">{{ message }}</div>
    {% endfor %}

    {% block content %}
    {% endblock content %}
    </div>

    <script src="https://cdn.datatables.net/t/bs-3.3.6/jq-2.2.0,dt-1.10.11,af-2.1.1/datatables.min.js"></script>

    {% block scripts %}
    {% endblock scripts %}

    {% include "fragments/footer.html" %}
</body>
</html>
